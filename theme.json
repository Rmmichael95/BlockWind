        {
          "name": "Medium",
          "slug": "medium",
          "size": "min(100%, calc(var(--wp--style--global--content-size) * 0.80))"
        },
        {
          "name": "Large",
          "slug": "large",
          "size": "min(100%, calc(var(--wp--style--global--content-size) * 0.90))"
        },

        {
          "name": "Sidebar",
          "slug": "sidebar",
          "size": "min(100%, 20rem)"
        },
        {
          "name": "Readable",
          "slug": "readable",
          "size": "min(100%, 60ch)"
        },

        {
          "name": "Content Width",
          "slug": "content",
          "size": "min(100%, var(--wp--style--global--content-size))"
        },
        {
          "name": "Section Width",
          "slug": "section",
          "size": "min(100%, calc(var(--wp--style--global--wide-size) * 0.70))"
        },
        {
          "name": "Extra Large",
          "slug": "extra-large",
          "size": "min(100%, calc(var(--wp--style--global--wide-size) * 0.80))"
