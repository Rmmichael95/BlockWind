    "typography": {
      "fontFamily": "var:preset|font-family|dh-body",
      "fontSize": "var:preset|font-size|fluid-default",
      "lineHeight": "1.6"
    },
    "border": {
      "color": "var:preset|color|dh-border"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var:preset|color|dh-link"
        },
        "typography": {
