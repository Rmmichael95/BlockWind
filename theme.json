        },
        "spacing": {
          "padding": {
            "left": "var:preset|spacing|6"
          },
          "margin": {
            "top": "var:preset|spacing|fluid-40",
            "bottom": "var:preset|spacing|fluid-40"
          }
        },
        "color": {
          "text": "var:preset|color|dh-text"
        },
        "elements": {
          "cite": {
            "color": {
