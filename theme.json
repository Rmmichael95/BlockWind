      },
      "core/site-tagline": {
        "typography": {
          "fontSize": "var:preset|font-size|sm"
        },
        "color": {
          "text": "var:preset|color|dh-muted"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var:preset|font-family|dh-heading",
          "fontWeight": "700",
          "fontSize": "var:preset|font-size|fluid-3xl",
          "lineHeight": "1.15"
        },
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|fluid-40",
            "bottom": "var:preset|spacing|4"
