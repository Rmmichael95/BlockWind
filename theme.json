    "dimensions": {
      "aspectRatio": true,
      "defaultAspectRatios": false,
      "aspectRatios": [
        {
          "name": "Square",
          "slug": "square",
          "ratio": "1 / 1"
        },
        {
          "name": "Standard",
          "slug": "standard",
          "ratio": "4 / 3"
        },
        {
          "name": "Landscape",
          "slug": "landscape",
          "ratio": "3 / 2"
        },
        {
          "name": "Portrait",
          "slug": "portrait",
          "ratio": "3 / 4"
        },
        {
          "name": "Widescreen",
          "slug": "widescreen",
          "ratio": "16 / 9"
        },
        {
          "name": "Ultra Wide",
          "slug": "ultra-wide",
          "ratio": "21 / 9"
