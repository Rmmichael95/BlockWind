      ],
      "fontSizes": [
        {
          "name": "Extra Small",
          "slug": "dh-xs",
          "size": "0.875rem",
          "fluid": { "min": "0.75rem", "max": "0.875rem" }
        },
        {
          "name": "Small",
          "slug": "dh-sm",
          "size": "1rem",
