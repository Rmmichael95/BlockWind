        ":focus": {
          "outline": {
            "color": "var:preset|color|dh-link",
            "style": "solid",
            "width": "2px",
            "offset": "2px"
          }
        }
      }
    },
    "blocks": {
      "core/paragraph": {
        "typography": {
          "fontSize": "var:preset|font-size|fluid-default"
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|fluid-40",
            "bottom": "var:preset|spacing|3"
          }
        }
      },
      "core/button": {
        "elements": {
          "link": {
