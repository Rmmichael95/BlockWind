{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "layout": {
      "contentSize": "640px",
      "wideSize": "1280px",
      "allowEditing": true,
      "allowCustomContentAndWideSize": false
    },
    "background": {
      "backgroundImage": true,
      "backgroundSize": true
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "radiusSizes": [
        {
          "name": "none",
          "slug": "dh-none",
          "size": "0px"
        },
        {
          "name": "Extra Small",
          "slug": "dh-xs",
          "size": "0.125rem"
        },
        {
          "name": "Small",
          "slug": "dh-sm",
          "size": "0.25rem"
        },
        {
          "name": "Medium",
          "slug": "dh-md",
          "size": "0.375rem"
        },
        {
          "name": "Large",
          "slug": "dh-lg",
          "size": "0.5rem"
        },
        {
          "name": "Extra Large",
          "slug": "dh-xl",
          "size": "0.75rem"
        },
        {
          "name": "2xl",
          "slug": "dh-2xl",
          "size": "1rem"
        },
        {
          "name": "3xl",
          "slug": "dh-3xl",
          "size": "1.5rem"
        },
        {
          "name": "4xl",
          "slug": "dh-4xl",
          "size": "2rem"
        },
        {
          "name": "Pill",
          "slug": "dh-pill",
          "size": "clamp(999px, 50vw, 9999px)"
        },
        {
          "name": "full",
          "slug": "dh-full",
          "size": "9999px"
        }
      ]
    },
    "color": {
      "defaultPalette": false,
      "palette": [
        {
          "name": "Primary",
          "slug": "dh-primary",
          "color": "#141e1c"
        },
        {
          "name": "Secondary",
          "slug": "dh-secondary",
          "color": "#1f2d2b"
        },
        {
          "name": "Base",
          "slug": "dh-base",
          "color": "#fffbef"
        },
        {
          "name": "Accent",
          "slug": "dh-accent",
          "color": "#d3a300"
        },
        {
          "name": "Link",
          "slug": "dh-link",
          "color": "#495c69"
        },
        {
          "name": "Text",
          "slug": "dh-text",
          "color": "#1e2326"
        },
        {
          "name": "Muted",
          "slug": "dh-muted",
          "color": "#565c57"
        },
        {
          "name": "Border",
          "slug": "dh-border",
          "color": "#d7d7d7"
        },
        {
          "name": "Contrast",
          "slug": "dh-contrast",
          "color": "#ffffff"
        },
        {
          "name": "Black",
          "slug": "dh-black",
          "color": "#000000"
        },
        {
          "name": "White",
          "slug": "dh-white",
          "color": "#ffffff"
        }
      ],
      "custom": true,
      "defaultGradients": false,
      "gradients": [],
      "customGradient": true,
      "defaultDuotone": false,
      "duotone": [],
      "customDuotone": true,
      "background": true,
      "text": true,
      "link": true,
      "heading": true,
      "button": true,
      "caption": true
    },
    "dimensions": {
      "aspectRatio": true,
      "defaultAspectRatios": false,
      "aspectRatios": [
        {
          "name": "Square",
          "slug": "square",
          "ratio": "1 / 1"
        },
        {
          "name": "Standard",
          "slug": "standard",
          "ratio": "4 / 3"
        },
        {
          "name": "Landscape",
          "slug": "landscape",
          "ratio": "3 / 2"
        },
        {
          "name": "Portrait",
          "slug": "portrait",
          "ratio": "3 / 4"
        },
        {
          "name": "Widescreen",
          "slug": "widescreen",
          "ratio": "16 / 9"
        },
        {
          "name": "Ultra Wide",
          "slug": "ultra-wide",
          "ratio": "21 / 9"
        },
        {
          "name": "Banner",
          "slug": "banner",
          "ratio": "3 / 1"
        },
        {
          "name": "Tall",
          "slug": "tall",
          "ratio": "4 / 5"
        },
        {
          "name": "Classic Portrait",
          "slug": "classic-portrait",
          "ratio": "2 / 3"
        }
      ],
      "height": true,
      "minHeight": true,
      "width": true,
      "dimensionSizes": [
        {
          "name": "Narrow",
          "slug": "dh-sm",
          "size": "min(100%, calc(var(--wp--style--global--content-size) * 0.60))"
        },
        {
          "name": "Medium",
          "slug": "dh-md",
          "size": "min(100%, calc(var(--wp--style--global--content-size) * 0.80))"
        },
        {
          "name": "Large",
          "slug": "dh-lg",
          "size": "min(100%, calc(var(--wp--style--global--content-size) * 0.90))"
        },

        {
          "name": "Sidebar",
          "slug": "dh-sidebar",
          "size": "min(100%, 20rem)"
        },
        {
          "name": "Readable",
          "slug": "dh-readable",
          "size": "min(100%, 60ch)"
        },

        {
          "name": "Content Width",
          "slug": "dh-content",
          "size": "min(100%, var(--wp--style--global--content-size))"
        },
        {
          "name": "Section Width",
          "slug": "dh-section",
          "size": "min(100%, calc(var(--wp--style--global--wide-size) * 0.70))"
        },
        {
          "name": "Extra Large",
          "slug": "dh-xl",
          "size": "min(100%, calc(var(--wp--style--global--wide-size) * 0.80))"
        },
        {
          "name": "Wide Width",
          "slug": "dh-wide",
          "size": "min(100%, var(--wp--style--global--wide-size))"
        },
        {
          "name": "Full Width",
          "slug": "dh-full",
          "size": "100%"
        }
      ]
    },
    "lightbox": {
      "enabled": true,
      "allowEditing": true
    },
    "position": {
      "sticky": true
    },
    "shadow": {
      "defaultPresets": false,
      "presets": [
        {
          "name": "None",
          "slug": "dh-none",
          "shadow": "none"
        },
        {
          "name": "2X Small",
          "slug": "dh-2xs",
          "shadow": "0 1px rgb(0 0 0 / 0.05)"
        },
        {
          "name": "Extra Small",
          "slug": "dh-xs",
          "shadow": "0 1px 2px 0 rgb(0 0 0 / 0.05)"
        },
        {
          "name": "Small",
          "slug": "dh-sm",
          "shadow": "0 1px 3px 0 rgb(0 0 0 / 0.1), 0 1px 2px -1px rgb(0 0 0 / 0.1)"
        },
        {
          "name": "Medium",
          "slug": "dh-md",
          "shadow": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)"
        },
        {
          "name": "Large",
          "slug": "dh-lg",
          "shadow": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)"
        },
        {
          "name": "Extra Large",
          "slug": "dh-xl",
          "shadow": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)"
        },
        {
          "name": "2X Large",
          "slug": "dh-2xl",
          "shadow": "0 25px 50px -12px rgb(0 0 0 / 0.25)"
        },
        {
          "name": "inner",
          "slug": "dh-inner",
          "shadow": "inset 0 2px 4px 0 rgb(0 0 0 / 0.05)"
        }
      ]
    },

    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": true,
      "customSpacingSize": true,
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "None",
          "slug": "dh-none",
          "size": "0px"
        },
        {
          "name": "Extra Small",
          "slug": "dh-xs",
          "size": "clamp(0.75rem, calc(0.7143rem + 0.1786vw), 0.875rem)"
        },

        {
          "name": "Small",
          "slug": "dh-sm",
          "size": "clamp(1.25rem, calc(1.1786rem + 0.3571vw), 1.5rem)"
        },
        {
          "name": "Medium",
          "slug": "dh-md",
          "size": "clamp(1.75rem, calc(1.6071rem + 0.7143vw), 2.25rem)"
        },
        {
          "name": "Large",
          "slug": "dh-lg",
          "size": "clamp(2rem, calc(1.7143rem + 1.4286vw), 3rem)"
        },
        {
          "name": "Extra Large",
          "slug": "dh-xl",
          "size": "clamp(2rem, calc(1.4286rem + 2.8571vw), 4rem)"
        },
        {
          "name": "2X Large",
          "slug": "dh-2xl",
          "size": "clamp(3rem, calc(2.1429rem + 4.2857vw), 6rem)"
        },
        {
          "name": "3X Large",
          "slug": "dh-3xl",
          "size": "clamp(4rem, calc(2.5714rem + 7.1429vw), 9rem)"
        }
      ],
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "typography": {
      "fluid": true,
      "defaultFontSizes": false,
      "customFontSize": false,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "lineHeight": true,
      "textIndent": "subsequent",
      "textAlign": true,
      "textColumns": false,
      "textDecoration": true,
      "writingMode": false,
      "textTransform": true,
      "dropCap": true,
      "fontFamilies": [
        {
          "name": "Body",
          "slug": "dh-body",
          "fontFamily": "ui-sans-serif, system-ui, -apple-system, \"Segoe UI\", Roboto, Helvetica, Arial, \"Apple Color Emoji\", \"Segoe UI Emoji\""
        },
        {
          "name": "Headings",
          "slug": "dh-heading",
          "fontFamily": "ui-sans-serif, system-ui, -apple-system, \"Segoe UI\", Roboto, Helvetica, Arial, \"Apple Color Emoji\", \"Segoe UI Emoji\""
        }
      ],
      "fontSizes": [
        {
          "name": "Extra Small",
          "slug": "dh-xs",
          "size": "0.875rem",
          "fluid": { "min": "0.75rem", "max": "0.875rem" }
        },
        {
          "name": "Small",
          "slug": "dh-sm",
          "size": "1rem",
          "fluid": { "min": "0.875rem", "max": "1rem" }
        },
        {
          "name": "Regular",
          "slug": "dh-md",
          "size": "1.125rem",
          "fluid": { "min": "1rem", "max": "1.125rem" }
        },
        {
          "name": "Large",
          "slug": "dh-lg",
          "size": "1.5rem",
          "fluid": { "min": "1.25rem", "max": "1.5rem" }
        },
        {
          "name": "Extra Large",
          "slug": "dh-xl",
          "size": "2.25rem",
          "fluid": { "min": "1.5rem", "max": "2.25rem" }
        },
        {
          "name": "Heading Small",
          "slug": "dh-heading-sm",
          "size": "3rem",
          "fluid": { "min": "2rem", "max": "3rem" }
        },
        {
          "name": "Heading Medium",
          "slug": "dh-heading-md",
          "size": "4rem",
          "fluid": { "min": "2.5rem", "max": "4rem" }
        },
        {
          "name": "Heading Large",
          "slug": "dh-heading-lg",
          "size": "5rem",
          "fluid": { "min": "3rem", "max": "5rem" }
        },
        {
          "name": "Heading Extra Large",
          "slug": "dh-heading-xl",
          "size": "6rem",
          "fluid": { "min": "3.5rem", "max": "6rem" }
        },
        {
          "name": "Heading Hero",
          "slug": "dh-heading-2xl",
          "size": "7.5rem",
          "fluid": { "min": "4rem", "max": "7.5rem" }
        },
        {
          "name": "Heading Hero Large",
          "slug": "dh-heading-3xl",
          "size": "9rem",
          "fluid": { "min": "4.5rem", "max": "9rem" }
        }
      ]
    },

    "custom": {
      "dh-line-height": {
        "body": "1.6",
        "heading": "1.25",
        "hero": "1.1",
        "tight": "1.15",
        "loose": "1.75"
      },
      "dh-font-weight": {
        "thin": "100",
        "extra-light": "200",
        "light": "300",
        "regular": "400",
        "medium": "500",
        "semi-bold": "600",
        "bold": "700",
        "extra-bold": "800",
        "black": "900"
      },
      "dh-measure": {
        "readable": "60ch"
      },
      "dh-focus-ring": {
        "width": "2px",
        "offset": "2px"
      },
      "dh-flow": {
        "gap": "var(--wp--preset--spacing--dh-md)",
        "gap-tight": "var(--wp--preset--spacing--dh-xs)",
        "section-padding-y": "var(--wp--preset--spacing--dh-2xl)",
        "content-padding-x": "var(--wp--preset--spacing--dh-sm)"
      },
      "dh-navigation": {
        "gap": "var(--wp--preset--spacing--dh-xs)",
        "item-padding-y": "var(--wp--preset--spacing--dh-xs)",
        "item-padding-x": "var(--wp--preset--spacing--dh-sm)",
        "dropdown-radius": "var(--wp--preset--radius--dh-md)",
        "dropdown-padding": "var(--wp--preset--spacing--dh-xs)",
        "border-width": "1px"
      }
    },
    "blocks": {}
  },
  "styles": {
    "css": "",
    "color": {
      "background": "var:preset|color|dh-base",
      "text": "var:preset|color|dh-text"
    },
    "spacing": {
      "blockGap": "var(--wp--custom--dh-flow--gap)",
      "padding": {
        "top": "var:preset|spacing|dh-none",
        "right": "var(--wp--custom--dh-flow--content-padding-x)",
        "bottom": "var:preset|spacing|dh-none",
        "left": "var(--wp--custom--dh-flow--content-padding-x)"
      }
    },
    "typography": {
      "fontFamily": "var:preset|font-family|dh-body",
      "fontSize": "var:preset|font-size|dh-md",
      "lineHeight": "var(--wp--custom--dh-line-height--body)"
    },
    "border": {
      "color": "var:preset|color|dh-border",
      "radius": "var:preset|radius|dh-md"
    },
    "elements": {
      "button": {
        "color": {
          "background": "var:preset|color|dh-accent",
          "text": "var:preset|color|dh-contrast"
        },
        "border": {
          "color": "var:preset|color|dh-border",
          "radius": "var:preset|radius|dh-md",
          "style": "solid",
          "width": "1px"
        },
        "spacing": {
          "padding": {
            "bottom": "var:preset|spacing|dh-xs",
            "left": "var:preset|spacing|dh-sm",
            "right": "var:preset|spacing|dh-sm",
            "top": "var:preset|spacing|dh-xs"
          }
        },
        "typography": {
          "fontWeight": "var(--wp--custom--dh-font-weight--semi-bold)",
          "textDecoration": "none"
        },
        ":hover": {
          "color": {
            "background": "var:preset|color|dh-primary",
            "text": "var:preset|color|dh-contrast"
          }
        },
        ":focus": {
          "outline": {
            "color": "var:preset|color|dh-link",
            "offset": "var(--wp--custom--dh-focus-ring--offset)",
            "style": "solid",
            "width": "var(--wp--custom--dh-focus-ring--width)"
          }
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var:preset|font-family|dh-heading",
          "fontWeight": "var(--wp--custom--dh-font-weight--semi-bold)",
          "lineHeight": "var(--wp--custom--dh-line-height--heading)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var:preset|font-size|dh-heading-md",
          "lineHeight": "var(--wp--custom--dh-line-height--hero)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|dh-heading-sm",
          "lineHeight": "var(--wp--custom--dh-line-height--tight)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var:preset|font-size|dh-xl",
          "lineHeight": "var(--wp--custom--dh-line-height--heading)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var:preset|font-size|dh-lg",
          "lineHeight": "var(--wp--custom--dh-line-height--heading)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var:preset|font-size|dh-md",
          "lineHeight": "var(--wp--custom--dh-line-height--heading)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var:preset|font-size|dh-sm",
          "lineHeight": "var(--wp--custom--dh-line-height--heading)"
        }
      },
      "link": {
        "color": {
          "text": "var:preset|color|dh-link"
        },
        "typography": {
          "textDecoration": "underline"
        },
        ":hover": {
          "typography": {
            "textDecoration": "underline"
          }
        },
        ":focus": {
          "outline": {
            "color": "var:preset|color|dh-link",
            "offset": "var(--wp--custom--dh-focus-ring--offset)",
            "style": "solid",
            "width": "var(--wp--custom--dh-focus-ring--width)"
          },
          "typography": {
            "textDecoration": "underline"
          }
        },
        ":active": {
          "color": {
            "text": "var:preset|color|dh-accent"
          }
        }
      }
    },
    "blocks": {
      "core/button": {
        "elements": {
          "link": {
            "typography": {
              "textDecoration": "none",
              "fontWeight": "var(--wp--custom--dh-font-weight--semi-bold)"
            }
          }
        },
        "border": {
          "radius": "var:preset|radius|dh-md"
        },
        "spacing": {
          "padding": {
            "top": "var:preset|spacing|dh-xs",
            "right": "var:preset|spacing|dh-sm",
            "bottom": "var:preset|spacing|dh-xs",
            "left": "var:preset|spacing|dh-sm"
          }
        },
        "css": ".wp-block-button.is-style-outline .wp-block-button__link{background:transparent;border:1px solid var(--wp--preset--color--dh-border);color:var(--wp--preset--color--dh-text)}.wp-block-button.is-style-outline .wp-block-button__link:hover,.wp-block-button.is-style-outline .wp-block-button__link:focus-visible{border-color:var(--wp--preset--color--dh-link)}"
      },
      "core/details": {
        "color": {
          "background": "var:preset|color|dh-contrast",
          "text": "var:preset|color|dh-text"
        },
        "border": {
          "color": "var:preset|color|dh-border",
          "style": "solid",
          "width": "1px",
          "radius": "var:preset|radius|dh-md"
        },
        "spacing": {
          "blockGap": "var(--wp--custom--dh-flow--gap)",
          "padding": {
            "top": "var:preset|spacing|dh-sm",
            "right": "var:preset|spacing|dh-sm",
            "bottom": "var:preset|spacing|dh-sm",
            "left": "var:preset|spacing|dh-sm"
          }
        },
        "typography": {
          "lineHeight": "var(--wp--custom--dh-line-height--body)"
        },
        "css": ".wp-block-details>summary{cursor:pointer;font-weight:var(--wp--custom--dh-font-weight--semi-bold)}.wp-block-details[open]>summary{margin-bottom:var(--wp--custom--dh-flow--gap)}.wp-block-details>summary:focus-visible{outline:var(--wp--custom--dh-focus-ring--width) solid var(--wp--preset--color--dh-link);outline-offset:var(--wp--custom--dh-focus-ring--offset)}"
      },
      "core/columns": {
        "spacing": {
          "blockGap": "var(--wp--custom--dh-flow--gap)"
        }
      },
      "core/cover": {
        "border": {
          "radius": "var:preset|radius|dh-lg"
        },
        "spacing": {
          "padding": {
            "bottom": "var:preset|spacing|dh-xl",
            "left": "var(--wp--custom--dh-flow--content-padding-x)",
            "right": "var(--wp--custom--dh-flow--content-padding-x)",
            "top": "var:preset|spacing|dh-xl"
          }
        }
      },
      "core/gallery": {
        "spacing": {
          "blockGap": "var:preset|spacing|dh-sm"
        }
      },
      "core/group": {
        "border": {
          "radius": "var:preset|radius|dh-lg"
        },
        "spacing": {
          "padding": {
            "bottom": "var:preset|spacing|dh-md",
            "left": "var:preset|spacing|dh-md",
            "right": "var:preset|spacing|dh-md",
            "top": "var:preset|spacing|dh-md"
          }
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "bottom": "var:preset|spacing|dh-xs",
            "top": "var(--wp--custom--dh-flow--gap)"
          }
        }
      },
      "core/image": {
        "border": {
          "radius": "var:preset|radius|dh-md"
        }
      },
      "core/list": {
        "spacing": {
          "padding": {
            "left": "var:preset|spacing|dh-md"
          }
        }
      },
      "core/media-text": {
        "spacing": {
          "blockGap": "var(--wp--custom--dh-flow--gap)"
        }
      },
      "core/navigation": {
        "typography": {
          "fontSize": "var:preset|font-size|dh-sm",
          "fontWeight": "var(--wp--custom--dh-font-weight--semi-bold)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|dh-text"
            },
            "typography": {
              "textDecoration": "none"
            },
            ":hover": {
              "color": {
                "text": "var:preset|color|dh-link"
              },
              "typography": {
                "textDecoration": "none"
              }
            },
            ":focus": {
              "outline": {
                "color": "var:preset|color|dh-link",
                "offset": "var(--wp--custom--dh-focus-ring--offset)",
                "style": "solid",
                "width": "var(--wp--custom--dh-focus-ring--width)"
