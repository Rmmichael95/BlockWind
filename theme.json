        "spacing": {
          "padding": {
            "top": "var:preset|spacing|2",
            "right": "var:preset|spacing|4",
            "bottom": "var:preset|spacing|2",
            "left": "var:preset|spacing|4"
          }
        },
        "color": {
          "background": "var:preset|color|dh-accent",
          "text": "var:preset|color|dh-contrast"
