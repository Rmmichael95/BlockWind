{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "title": "Dh Everforest Light",
  "settings": {
    "color": {
      "palette": [
        {
          "name": "BW Primary",
          "slug": "bw-primary",
          "color": "#495c69"
        },
        {
          "name": "BW Secondary",
          "slug": "bw-secondary",
          "color": "#97a47f"
        },
        {
          "name": "BW Base",
          "slug": "bw-base",
          "color": "#fffbef"
        },
        {
          "name": "BW Accent",
          "slug": "bw-accent",
          "color": "#d3a300"
        },
        {
          "name": "BW Link",
          "slug": "bw-link",
          "color": "#495c69"
        },
        {
          "name": "BW Text",
          "slug": "bw-text",
          "color": "#1E2326"
        },
        {
          "name": "BW Muted",
          "slug": "bw-muted",
          "color": "#565c57"
        },
        {
          "name": "BW Border",
          "slug": "bw-border",
          "color": "#d7d7d7"
        },
        {
          "name": "BW Contrast",
          "slug": "bw-contrast",
          "color": "#ffffff"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "name": "Body",
          "slug": "bw-body",
          "fontFamily": "ui-sans-serif, system-ui, -apple-system, \"Segoe UI\", Roboto, Helvetica, Arial, \"Apple Color Emoji\", \"Segoe UI Emoji\""
        },
        {
          "name": "Headings",
          "slug": "bw-heading",
          "fontFamily": "ui-sans-serif, system-ui, -apple-system, \"Segoe UI\", Roboto, Helvetica, Arial, \"Apple Color Emoji\", \"Segoe UI Emoji\""
        }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var:preset|color|bw-base",
      "text": "var:preset|color|bw-text"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var:preset|color|bw-link"
        }
      },
      "button": {
        "color": {
          "background": "var:preset|color|bw-primary",
          "text": "var:preset|color|bw-contrast"
        }
      }
    }
  }
}
